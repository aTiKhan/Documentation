{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*/src/**.csproj"
          ],
          "src": "../Steeltote/src"
        }
      ],
      "dest": "apidoc",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net5.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**/toc.yml",

          "apidoc/**.yml",

          "articles/**.md",
          "articles/**/toc.yml",

          "labs/**.md",
          "labs/**/toc.yml",
          
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "fonts/**",
          "articles/**/images/**",
          "api/**/images/**",
          "labs/**/images/**",
          "**.html"
        ]
      }
    ],
    "xref": [
      "_site/xrefmap.yml"
    ],
    "dest": "_site",
    "globalMetadataFiles": ["metadata.json"],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template/steeltoe"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}